            viewModel._mestruazioniCessazioneAltro("");
            viewModel._mestruazioniCessazioneAltroMotivazione("");
        }
        if (v == "3") {
            viewModel._mestruazioniCessazioneTipoIntervento("");
            viewModel._mestruazioniCessazioneMotivazione("");
        }
    });

    viewModel._gravidanze.subscribe(function (v) {
        if (v == "1") {
            viewModel._gravidanzeTentativi(false);
            viewModel._gravidanzeCureOrmonali(false);
        }
        if (v == "2") {
            viewModel._gravidanzeNumero(false);
            viewModel._gravidanzeConcluse("");
            viewModel._gravidanzeEtaPrimofiglio("");
            //viewModel._gravidanzeEtaUltimofiglio("");
        }
    });

    viewModel._terapiaOrmonale.subscribe(function (v) {
