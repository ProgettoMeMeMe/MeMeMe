            viewModel._apAltreTxt("");
        }
    });

    viewModel._menopausa.subscribe(function (v) {
        if (v == 2) {
            viewModel._mestruazioniDataUltima("");
            viewModel._mestruazioniDataUltimaNsNr(true);
            viewModel._menopausaEta("");
            viewModel._mestruazioniCessazione(false);
            viewModel._mestruazioniCessazioneEta("");
        }
        if (v == 1) {
            viewModel._mestruazioniDataUltima("");
            viewModel._mestruazioniDataUltimaNsNr(false);
        }

    });

    //viewModel._mestruazioniDataUltima.subscribe(function (v) {
    //    if (v == "" && viewModel._mestruazioniDataUltimaNsNr() == false) {
    //        viewModel._menopausaEta("");
    //        viewModel._mestruazioniCessazione(false);
    //        viewModel._mestruazioniCessazioneEta("");
    //    } else {
    //        viewModel._mestruazioniDataUltimaNsNr(false);
    //    }
    //
    //});

    viewModel._mestruazioniDataUltima.subscribe(function (v) {
        if (v != "") {
            viewModel._mestruazioniDataUltimaNsNr(false);
        }

    });


    viewModel._mestruazioniDataUltimaNsNr.subscribe(function (v) {
        if (v) {
            viewModel._mestruazioniDataUltima('');
