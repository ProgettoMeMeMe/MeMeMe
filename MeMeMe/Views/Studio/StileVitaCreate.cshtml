            viewModel._terapiaOrmonaleTempo("");
        }
    });
    viewModel._pillolaContraccettiva.subscribe(function (v) {
        if (v != "1") {
            viewModel._pillolaContraccettivaTempo("");
        }
    });

    $(document).ready(function () {
        ko.applyBindings(viewModel);
        jQuery.validator.addMethod(
            'date',
            function (value, element, params) {
                if (this.optional(element)) {
                    return true;
                };
                var result = false;
                try {
                    $.datepicker.parseDate('dd/mm/yy', value);
                    result = true;
                } catch (err) {
                    result = false;
                }
                return result;
            },
            ''
        );
    });
</script>



<h2>Stile di Vita @ViewBag.Paziente</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true)
        @Html.HiddenFor(model => model.idStatoRegistro, new { @Value = ViewBag.idStatoRegistro })

        <div class="form-group required">
            @Html.LabelFor(model => model.data, new { @class = "control-label col-md-4" })
            <div class="col-xs-2">
                @Html.EditorFor(model => model.data, new { htmlAttributes = new { @class = "form-control", data_bind = "value: _data" } })
                @Html.ValidationMessageFor(model => model.data)
            </div>
        </div>

        <div class="sezione">
            <h3 class="text-center">Motivazioni</h3>
            <div class="col-md-8">
                <label class="checkbox-inline">
                    @Html.CheckBoxFor(model => model.mPreoccupazione)
                    @Html.LabelFor(model => model.mPreoccupazione, new { style = "font-weight:normal" })
                </label>
                <label class="checkbox-inline">
                    @Html.CheckBoxFor(model => model.mRicerca)
                    @Html.LabelFor(model => model.mRicerca, new { style = "font-weight:normal" })
                </label>
                <label class="checkbox-inline">
                    @Html.CheckBoxFor(model => model.mStile)
                    @Html.LabelFor(model => model.mStile, new { style = "font-weight:normal" })
                </label>
                <label class="checkbox-inline">
                    @Html.CheckBoxFor(model => model.mAltro, new { data_bind = " checked : _mAltro" })
                    @Html.LabelFor(model => model.mAltro, new { style = "font-weight:normal" })
                </label>
            </div>
            @Html.TextBoxFor(model => model.motivazioneAltro, new { data_bind = "value: _motivazioneAltro, enable: _mAltro()==true", @class = "form-control" })
            @Html.ValidationMessageFor(model => model.motivazioneAltro)
        </div>

        <div class="sezione">
            <h3 class="text-center">Fumo</h3>
            <div class="form-group">
                @Html.LabelFor(model => model.fumo, new { @class = "control-label col-md-4" })
                <div class="form col-md-7">
                    <div class="radio-inline">
                        <label style="font-weight:normal">
                            @Html.RadioButtonFor(model => model.fumo, 1, new { data_bind = " checked : _fumo" })
                            No, non ho mai fumato
                        </label>
                    </div>
                    <div class="radio-inline">
                        <label style="font-weight:normal">
                            @Html.RadioButtonFor(model => model.fumo, 2, new { data_bind = " checked : _fumo" })
                            No, ho smesso all’età di:
                        </label>
                    </div>
                    <div class="radio-inline a">
                        @Html.TextBoxFor(model => model.fumoFine, new { data_bind = "value: _fumoFine, enable: _fumo()==2", @class = "form-control", @style = "width:50px;", @maxlength = "2" })
                        @Html.ValidationMessageFor(model => model.fumoFine)
                    </div>
                    <div class="radio-inline">
                        <label style="font-weight:normal">
                            @Html.RadioButtonFor(model => model.fumo, 3, new { data_bind = " checked : _fumo" })
                            Si, fumo
                        </label>
                    </div>
                    @Html.HiddenFor(model => model.fumo, null)
                    @Html.ValidationMessageFor(model => model.fumo)
                </div>
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.fumoInizio, new { @class = "control-label col-md-4" })
                <div class="col-md-1">
                    @Html.TextBoxFor(model => model.fumoInizio, null, new { data_bind = "value: _fumoInizio, enable: _fumo()!=1", @class = "form-control", @style = "width:50px;", @maxlength = "2" })
                    @Html.ValidationMessageFor(model => model.fumoInizio)
                </div>
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.fumoSigaretteMedia, new { @class = "control-label col-md-4" })
                <div class="col-md-1">
                    @Html.TextBoxFor(model => model.fumoSigaretteMedia, null, new { data_bind = "value: _fumoSigaretteMedia, enable: _fumo()!=1", @class = "form-control", @style = "width:50px;", @maxlength = "2" })
                    @Html.ValidationMessageFor(model => model.fumoSigaretteMedia)
                </div>
            </div>

            <div class="form-group">
                @Html.LabelFor(model => model.fumoSigaretteMax, new { @class = "control-label col-md-4" })
                <div class="col-md-1">
                    @Html.TextBoxFor(model => model.fumoSigaretteMax, null, new { data_bind = "value: _fumoSigaretteMax, enable: _fumo()!=1", @class = "form-control", @style = "width:50px;", @maxlength = "2" })
                    @Html.ValidationMessageFor(model => model.fumoSigaretteMax)
                </div>
            </div>

