                    @Html.HiddenFor(model => model.stitichezza, null)
                    @Html.ValidationMessageFor(model => model.stitichezza)
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(model => model.stitichezzaLassativi, new { @class = "control-label col-md-4" })
                <div class="form col-md-7">
                    <div class="radio-inline">
                        <label style="font-weight:normal">
                            @Html.RadioButtonFor(model => model.stitichezzaLassativi, 1, new { data_bind = "checked : _stitichezzaLassativi, enable: _stitichezza()==1" })
                            mai
                        </label>
                    </div>
                    <div class="radio-inline">
                        <label style="font-weight:normal">
                            @Html.RadioButtonFor(model => model.stitichezzaLassativi, 2, new { data_bind = "checked : _stitichezzaLassativi, enable: _stitichezza()==1" })
                            raramente
                        </label>
                    </div>
                    <div class="radio-inline">
                        <label style="font-weight:normal">
                            @Html.RadioButtonFor(model => model.stitichezzaLassativi, 3, new { data_bind = "checked : _stitichezzaLassativi, enable: _stitichezza()==1" })
                            più volte a settimana
                        </label>
                    </div>
                    @Html.HiddenFor(model => model.stitichezzaLassativi, null)
                    @Html.ValidationMessageFor(model => model.stitichezzaLassativi)
                </div>
            </div>
            @if (ViewBag.sesso == "M")
            {

                <div class="form-group">
                    @Html.Label("Quale è il suo peso massimo raggiunto", new { @class = "control-label col-md-4" })
                    <div class="col-md-5">
                        @Html.TextBoxFor(model => model.pesoMax, null, new { @class = "form-control", @style = "width:60px;", @maxlength = "3" })
                        @Html.ValidationMessageFor(model => model.pesoMax)
                    </div>
                </div>
            }
        </div>
        <br />
            <div class="form-group">
                <div class="col-md-offset-4 col-md-8">
                    @if (User.IsInRole("audit"))
                    {
                        <input type="submit" value="Registra" class="btn btn-default" disabled />
                    }
                    else
                    {
                        <input type="submit" value="Registra" class="btn btn-default" />
                    }
                </div>
            </div>
        </div>
    }