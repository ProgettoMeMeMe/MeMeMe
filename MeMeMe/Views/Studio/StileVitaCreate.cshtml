            viewModel._mestruazioniCessazioneTipoIntervento("");
            viewModel._mestruazioniCessazioneMotivazione("");
            viewModel._mestruazioniCessazioneAltro("");
            viewModel._mestruazioniCessazioneAltroMotivazione("");
