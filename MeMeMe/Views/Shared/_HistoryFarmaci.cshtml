@model IEnumerable<MeMeMe.Models.FarmaciListato>
@{ Layout = null; }

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.consegna)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fase)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dosaggio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PeriodoDaTmeno1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.flaconi)
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @*@Html.ActionLink(item.consegna.ToShortDateString(), "../Studio/AssegnazioneFarmacoEdit", new { id = item.idAssegnazioneRegistro })*@
                    @Html.DisplayFor(modelItem => item.consegna)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.fase)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.dosaggio)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PeriodoDaTmeno1)
                </td>
                <td>
                    @*Html.DisplayFor(modelItem => item.flaconi)*@
                    @Html.ActionLink(item.flaconi, "StampaModulo", new { id = item.idAssegnazioneRegistro })
                </td>
            </tr>
        }

    </table>
